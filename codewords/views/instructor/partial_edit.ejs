
<!-- main  -->


      <div class="form-group">
        <label for="coursename">CourseName</label>
        <input type="text" class="form-control" id="coursename" autocomplete="off" name="coursename" value="<%= instructors.coursename %>" required>
      </div>
      <div class="form-group">
        <label for="startdate">StartDate</label>
        <input type="text" class="form-control" id="startdate" autocomplete="off" name="startdate" value="<%= instructors.startdate %>" required>
      </div>
      <div class="form-group">
        <label for="enddate">EndDate</label>
        <input type="text" class="form-control" id="enddate" autocomplete="off" name="enddate" value="<%= instructors.enddate %>" required>     
    </div>
    <div class="form-group">
      <label for="intiallink">IntialLink</label>
      <input type="text" class="form-control" id="intiallink" autocomplete="off" name="intiallink" value="<%= instructors.intiallink %>" required>
    </div>
    <div class="form-group">
      <label for="finallink">FinalLink</label>
      <input type="text" class="form-control" id="finallink" autocomplete="off" name="finallink" value="<%= instructors.finallink %>" required>
    </div>
    <div class="form-group">
      <label for="studentlist">Assign StudentList</label>
      <input type="file" class="form-control" id="studentlist" autocomplete="off" name="studentlist" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet">
    </div>
    <div class="form-group">
      
      <select class="form-control form-control-lg" id="codeword" autocomplete="off" name="codeword">
          <option value="">Code Word</option>
          <option value="Fruits">Fruits</option>
          <option value="Cities">Cities</option>
          <option value="Cars">Cars</option>
        </select>
        
      <!-- <input type="text" class="form-control" id="codewordsetname" name="codewordsetname" value="<%= instructors.codewordsetname %>" required> -->
    </div>

  <!-- <% include ../footer %> -->
  <script>
    $( function() {
      $( "#startdate" ).datepicker();
      $( "#enddate" ).datepicker();
    } );
    </script>
 